{
  "manifest_version": 3,
  "name": "Phishing Link Detector",
  "version": "1.0",
  "description": "Detects phishing links in URLs.",
  "permissions": ["storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
  },
  "background": {
      "service_worker": "background.js"
  }
}

